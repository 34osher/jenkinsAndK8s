spec:
  ingressClassName: nginx
  rules:
  - host: for-k8s-eabb40b337d19829.elb.eu-west-3.amazonaws.com
    http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service: 
            name: jenkins-service
            port:
              number: 8080